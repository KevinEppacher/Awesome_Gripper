<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="gripper">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import gripper urdf file -->
    <xacro:include filename="$(find gripper)/urdf/gripper.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="gripper.ros2_control.xacro" />


    <xacro:gripper_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
